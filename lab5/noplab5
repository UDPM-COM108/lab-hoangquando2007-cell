/*
#include <stdio.h>
#include <math.h>

//XÂY DỰNG CHƯƠNG TRÌNH TÌM GIÁ TRỊ LỚN NHẤT, NHỎ NHẤT TRONG 3 SỐ
int min3(int a, int b, int c) {
    int m = (a < b) ? a : b;
    return (m < c) ? m : c;
}

int max3(int a, int b, int c){
    int m = (a > b) ? a : b;
    return (m > c) ? m : c;
}
int abs(int x) {
    return (x<0) ? -x:x;
}
int main(){
    int a,b,c;
    printf("\nNhập giá trị a: ");
    scanf("%d",&a);
    printf("\nNhập giá trị b: ");
    scanf("%d",&b);
    printf("\nNhập giá trị c: ");
    scanf("%d",&c);
    int min = min3(a,b,c);
    int max = max3(a,b,c);
    printf("Giá trị nhỏ nhất: %d\n",min);
    printf("Giá trị lớn nhất: %d\n",max);
