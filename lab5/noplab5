#include <stdio.h>

int main(){
    int n;
    printf("Nhập số từ 1 đến 1000: ");
    scanf("%d", &n);
    if (n<1 || n>1000)
    {
        printf("Số không nằm trong khoảng 1-1000: \n");
    }
    if (n%5 == 0 && n%9 ==0)
    {
        printf("%d chia hết cho 5 và 9\n", n);
    }else{
        printf("%d không chia hết cho 5 và 9", n);
    }
    return 0;
}
